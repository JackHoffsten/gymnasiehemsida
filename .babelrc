{
    "presets": [
      "@babel/react",
      "@babel/typescript",
      [
        "@babel/env",
        {
          "modules": false,
          "useBuiltIns": "usage",
          "targets": {
            "browsers": [
              "Chrome >= 52",
              "FireFox >= 44",
              "Safari >= 7",
              "Explorer 11",
              "last 4 Edge versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
    ]
  }