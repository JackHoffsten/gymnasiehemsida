{
    "extends": ["tslint-react", "tslint-microsoft-contrib"],
    "rulesDirectory": ["src/", "node_modules/tslint-microsoft-contrib"],
    "rules": {
      "max-line-length": {
        "options": [120]
      },
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false,
      "jsx-no-multiline-js": false,
      "prefer-const": true,
      "cyclomatic-complexity": false,
      "quotemark": [
        true,
        "single",
        "jsx-double",
        "avoid-escape",
        "avoid-template"
      ],
      "no-console": {
        "severity": "warning",
        "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
      },
      "curly": true,
      "radix": true,
      "triple-equals": true,
      "semicolon": [true, "always"],
      "no-relative-imports": [true, "allow-siblings"],
      "interface-name": false,
      "no-any": false,
      "typedef": false,
      "no-null-keyword": false,
      "use-simple-attributes": false,
      "no-single-line-block-comment": false,
      "no-implicit-dependencies": false,
      "no-submodule-imports": false,
      "variable-name": [true, "check-format", "allow-pascal-case"],
      "prefer-method-signature": false,
      "max-func-body-length": false,
      "react-a11y-anchors": false,
      "ordered-imports": false,
      "newline-per-chained-call": false,
      "react-a11y-event-has-role": false,
      "react-a11y-role-has-required-aria-props": false,
      "no-backbone-get-set-outside-model": false,
      "array-type": false,
      "prefer-array-literal": false,
      "no-suspicious-comment": false,
      "import-name": false,
      "newline-before-return": false,
      "align": false,
      "no-increment-decrement": false,
      "react-a11y-input-elements": false,
      "react-a11y-no-onchange": false,
      "no-cookies": false,
      "await-promise": false,
      "match-default-export-name": false,
      "no-floating-promises": false,
      "no-for-in-array": false,
      "no-unsafe-any": false,
      "no-use-before-declare": false,
      "promise-function-async": false,
      "restrict-plus-operands": false,
      "strict-boolean-expressions": false,
      "completed-docs": false,
      "no-unnecessary-qualifier": false,
      "no-unnecessary-type-assertion": false,
      "no-void-expression": false,
      "prefer-readonly": false,
      "use-default-type-parameter": false,
      "no-import-side-effect": false,
      "no-reserved-keywords": false,
      "no-default-export": false,
      "jsx-wrap-multiline": false
    },
    "jsRules": {
      "max-line-length": {
        "options": [120]
      }
    }
  }
  